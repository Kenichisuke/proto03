<h2>Make Multipule Open Orders</h2>




  <%= form_for( @open_order ) do |f| %>
    <div class="form-group">
      <%= f.label :user_id %>
      <%= f.collection_select(:user_id, @admins,  :id, :email) %>
    </div>
    <div class="form-group">
      <%= f.label :coin1 %>
      <%= f.collection_select(:coin1, @cointypes,  :id, :ticker) %>
    </div>
    <div class="form-group">
      <%= f.label :coin2 %>
      <%= f.collection_select(:coin2, @cointypes,  :id, :ticker) %>
    </div>
    <div class="form-group">
      <%= f.label :pr %>
      <%= f.number_field :pr, min: 1, step: 0.1 %>
    </div>
    <div class="form-group">
      <%= f.label :steprate %>
      <%= f.number_field :steprate, min: 1, step: 0.1 %>
    </div>
    <div class="form-group">
      <%= f.label :num %>
      <%= f.number_field :num, min: 1, step: 1, max: 20 %>
    </div>
    <div class="form-group">
      <%= f.label :eachamt %>
      <%= f.number_field :eachamt, min: 0 %>
    </div>
    <div class="form-group">
      <%= f.label :updown %>
      <%= f.number_field :updown, min: 0, step: 1, max: 2 %>
    </div>

    <%= f.submit 'Submit' %>
  <% end %>
